{
  "name": "qics",
  "version": "1.0.0",
  "description": "Library Description",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/qics.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/qics.min.js",
    "build:all": "npm run build && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test examples",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamisntdead/qics.git"
  },
  "keywords": [
    "Quantum",
    "Computing",
    "Simulator",
    "Emulator",
    "Matrix",
    "Ideal"
  ],
  "author": "Adam Kelly",
  "license": "Apache 2.0",
  "tonicExampleFilename": "examples/node.js",
  "bugs": {
    "url": "https://github.com/adamisntdead/qics/issues"
  },
  "homepage": "https://github.com/adamisntdead/qics",
  "dependencies": {
    "weighted": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.5.3",
    "eslint": "^3.1.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^6.2.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14"
  }
}
